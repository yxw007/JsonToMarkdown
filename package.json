{
  "name": "jsontomarkdown",
  "displayName": "JsonToMarkdown",
  "description": "json translate markdown table",
  "version": "0.0.1",
  "author": "yxw007<aa4790139@gmail.com>",
  "repository": "https://github.com/yxw007/JsonToMarkdown.git",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "yxw007",
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "editor.jsonToMarkdownTable",
        "title": "json to markdown table"
      }
    ],
    "keybindings": [
      {
        "command": "editor.jsonToMarkdownTable",
        "key": "ctrl+shift+alt+t"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "build": "npx vsce package",
    "release": "npx changelogen --release -i",
    "test": "vscode-test",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts}": [
      "npm lint"
    ]
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.5.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.87.0",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "changelogen": "^0.5.7",
    "commitlint": "^19.5.0",
    "eslint": "^8.56.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "json5": "^2.2.3",
    "vsce": "^2.15.0"
  }
}
